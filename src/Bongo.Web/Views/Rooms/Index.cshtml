@model IEnumerable<StudyRoom>

<br />
<div class="backgroundWhite">
            <h2 class="text-info"> Room List</h2>
    <br />
    <div>
        @if (Model.Count() > 0)
        {
            <table class="table table-striped border">
                <tr class="table-secondary">
                    <th>
                        Id
                    </th>
                    <th>
                        Room Number
                    </th>
                    <th>
                        Room Name
                    </th>
                   
                </tr>
                @foreach (var item in Model)
                {
            <tr>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.RoomNumber
                </td>
                <td>
                    @item.RoomName
                </td>
                
            </tr>
                }
            </table>
        }
        else
        {
            <p>No category exists...</p>
        }
    </div>
</div>